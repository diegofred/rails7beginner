<%= link_to products_path do %>
  <h1 class="text-red-400 pt-4 hover:text-blue-400 animate-bounce dark:text-white">My Store</h1>
<% end %>
<%= link_to 'Add Product', new_product_path  if Current.user%>
<%= link_to t('.categories'), categories_path if Current.user %>
<%= link_to t('.login'), new_session_path unless Current.user%>
<%= link_to t('.register'), new_user_path unless Current.user%>
<%= link_to t('.logout'), session_path(Current.user.id), data: {
  turbo_method: :delete,
  turbo_confirm: t('common.confirm')
} if Current.user %>
